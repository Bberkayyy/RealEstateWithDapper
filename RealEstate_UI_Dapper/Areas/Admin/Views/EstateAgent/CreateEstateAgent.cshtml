@model AdminPanelCreateEstateAgentViewModel
@{
    ViewData["Title"] = "CreateEstateAgent";
    Layout = "~/Areas/Admin/Views/Default/Index.cshtml";
}

<div class="col-sm-12 col-xl-12">
    <div class="bg-light rounded h-100 p-4">
        <h6 class="mb-4">Emlakçı Ekleme Sayfası</h6>
        <form method="post">
            <div class="mb-3">
                <label class="form-label">Adı Soyadı</label>
                <input type="text" class="form-control" asp-for="FullName" />
            </div>
            <div class="mb-3">
                <label class="form-label">Ünvanı</label>
                <input type="text" class="form-control" asp-for="Title" />
            </div>
            <div class="mb-3">
                <label class="form-label">Mail Adresi</label>
                <input type="text" class="form-control" asp-for="Mail" />
            </div>
            <div class="mb-3">
                <label class="form-label">Telefon Numarası</label>
                <input type="text" class="form-control" asp-for="PhoneNumber" />
            </div>
            <div class="mb-3">
                <label class="form-label">Resim</label>
                <input type="text" class="form-control" asp-for="ImageUrl" />
            </div>
            <button type="submit" class="btn btn-primary">Kaydet</button>
        </form>
    </div>
</div>

