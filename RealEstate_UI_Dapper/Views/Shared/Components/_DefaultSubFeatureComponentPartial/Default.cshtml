@model List<ResultSubFeatureViewModel>
<section class="w3l-bottom-grids py-5" id="steps">
    <div class="container py-lg-5 py-md-4 py-2">
        <div class="grids-area-hny main-cont-wthree-fea row">
            @foreach (var item in Model)
            {
                <div class="col-lg-4 col-md-6 grids-feature mt-md-0 mt-4">
                    <div class="area-box no-box-shadow text-left">
                        <span class="@item.icon"></span>
                        <h5>@item.topTitle</h5>
                        <h4><a href="#feature" class="title-head">@item.title</a></h4>
                        <p>@item.description</p>
                        <a href="#more" class="more">@item.subTitle <span class="fa fa-long-arrow-right"></span> </a>
                    </div>
                </div>
            }
        </div>
    </div>
</section>